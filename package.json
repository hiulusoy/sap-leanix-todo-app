{
  "name": "sap-leanix-todo-server",
  "version": "0.0.1",
  "description": "sap-leanix-todo - Server",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "echo 'Skipping postinstall during Docker build'",
    "clean": "echo 'Implement clean script if needed'",
    "client:build": "cd client && npm run build",
    "build": "tsc",
    "build-all": "npm run client:build && npm run build",
    "start": "npm run build-all && nodemon dist/server.js",
    "start-server": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server/server.ts",
    "test": "jest --coverage"
  },
  "engines": {
    "npm": "10.8.1",
    "node": "21"
  },
  "keywords": [
    "sap-leanix-todo"
  ],
  "author": "hiulusoy",
  "license": "COMMERCIAL",
  "dependencies": {
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.21.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.9.1",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21",
    "winston": "^3.14.2"
  },
